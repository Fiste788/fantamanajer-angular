{
  "rulesDirectory": [
    "node_modules/codelyzer",
    "node_modules/tslint-origin-ordered-imports-rule/dist"
  ],
  "extends": [
    "angular-tslint-rules"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "fm",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "fm",
      "kebab-case"
    ],
    "template-cyclomatic-complexity": [
      true,
      8
    ],
    "i18n": [
      false
    ],
    "template-i18n": [
      false,
      "check-id",
      "check-text"
    ],
    "no-implicit-dependencies": [
      true,
      [
        "@app",
        "@modules",
        "@shared",
        "@env",
        "protractor"
      ]
    ],
    "origin-ordered-imports": [
      true,
      "one-blank-line",
      [
        "lib",
        "^@app/.+|^@shared/.+|^@modules/.+|^@env",
        "user"
      ]
    ]
  }
}
