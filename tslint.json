{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": [
    "angular-tslint-rules"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "fm",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "fm",
      "kebab-case"
    ],
    "i18n": [
      false
    ],
    "template-i18n": [
      false,
      "check-id",
      "check-text"
    ],
    "no-implicit-dependencies": [
      true,
      [
        "@app",
        "@env"
      ]
    ]
  }
}
